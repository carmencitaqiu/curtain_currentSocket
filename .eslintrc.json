{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "eqeqeq": "warn",
    "indent": "off",
    "semi": ["error", "always"]
  },
  "globals": {
    "autor": true,
    "Lcp": true
  }
}
